{
    "version": 2,
    "builds": [
        {
          "src": "index.py",
          "use": "@vercel/python"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/index.py"
        }
      ],
      "framework": {
        "name": "python",
        "version": "3.9"
      }
    }